actor main(env):
    var exp = ["--", "foo", "--bar", "--rts-verbose"]
    var i = 0
    print("Expected:", exp)
    print("env.argv:", env.argv[1:])

    for arg in env.argv[1:]:
        if arg != exp[i]:
            print("Oh noes:", arg, "!=", exp[i])
            exit(env.cap, 1)
        i += 1

    i = 1
    for ex in exp:
        if ex != env.argv[i]:
            print("Oh noes:", ex, "!=", env.argv[i])
            exit(env.cap, 1)
        i += 1

    print("All is well")
    exit(env.cap, 0)
