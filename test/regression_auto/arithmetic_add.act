actor main(env):
    def testEqual(a, b, msg):
        if a == b:
            print(msg, "=", a, "as expected (", b, ")")
        if a != b:
            print("Assertion error:", a, "!=", b, "(" + msg + ")")
            exit(env.cap, 1)

    testEqual(0+1, 1, "0+1")
    testEqual(1+2, 3, "1+2")

    exit(env.cap, 0)
