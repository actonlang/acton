actor main(env):
    def testEqual(a, b, msg):
        if a == b:
            print(msg, "=", a, "as expected (", b, ")")
        if a != b:
            print("Assertion error:", a, "!=", b, "(" + msg + ")")
            exit(env.cap, 1)

    testEqual(5-3, 2, "5-3")
    testEqual(432-75, 357, "432-75")

    exit(env.cap, 0)
