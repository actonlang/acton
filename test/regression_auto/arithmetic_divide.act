actor main(env):
    def testEqual(a : float, b : float, msg):
        if a == b:
            print(msg, "=", a, "as expected (", b, ")")
        if a != b:
            print("Assertion error:", a, "!=", b, "(" + msg + ")")
            exit(env.cap, 1)
    testEqual(8/2, 4, "8/2")
    testEqual(32/4, 8, "32/4")
    exit(env.cap, 0)
