actor main(env):
    def testEqual(a, b, msg):
        if a == b:
            print(msg, "=", a, "as expected (", b, ")")
        if a != b:
            print("Assertion error:", a, "!=", b, "(" + msg + ")")
            exit(env.cap, 1)

    testEqual(4*5, 20, "4*5")
    testEqual(112*3, 336, "112*3")

    exit(env.cap, 0)
