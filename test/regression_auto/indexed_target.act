class tree():
    def __init__(self, children : list[tree], data : int):
        self.children = children
        self.data = data

def getchild(self : tree, n : int) -> tree:
    return self.children[n]

def setchild(self : tree, n : int, t : tree) -> None:
    self.children[n] = t

def delchild(self : tree, n : int) -> None:
    del self.children[n]

actor main(env):
    exit(env.cap, 0)
