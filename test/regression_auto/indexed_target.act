class tree():
    def __init__(self, children : list[tree], data : int):
        self.children = children
        self.data = data

def getchild(self : tree, n : i64) -> tree:
    return self.children[n]

def setchild(self : tree, n : i64, t : tree) -> None:
    self.children[n] = t

def delchild(self : tree, n : i64) -> None:
    del self.children[n]

actor main(env):
    env.exit(0)
