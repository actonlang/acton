class Gamma():
    def __init__(self, s: ?str):
        self.s = s

class Beta():
    def __init__(self, c: ?Gamma):
        self.c = c

class Alpha():
    def __init__(self, b: ?Beta):
        self.b = b

actor main(env):
    a1 = None
    print(a1?.b?.c?.s)

    a2 = Alpha(Beta(None))
    print(a2?.b?.c?.s)

    a3 = Alpha(Beta(Gamma("hello")))
    print(a3?.b?.c?.s)

    env.exit(0)
