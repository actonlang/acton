class Apa ():
    def __init__(self, bepas: list[Bepa], cepas: list[Cepa]):
        self.bepas = bepas
        self.cepas = cepas

class Bepa ():
    def __init__(self, apas: list[Apa]):
        self.apas = apas

class Cepa ():
    def __init__(self, apas: list[Apa]):
        self.apas = apas
    
extension Apa (Ord):
    def __eq__(x, y):
        return x.bepas == y.bepas and x.cepas == y.cepas
    def __lt__(x, y):
        return x.bepas < y.bepas and x.cepas < y.cepas

extension Bepa (Ord):
    def __eq__(x, y):
        return x.apas == y.apas
    def __lt__(x, y):
        return x.apas < y.apas

extension Cepa (Ord):
    def __eq__(x, y):
        return x.apas == y.apas
    def __lt__(x, y):
        return x.apas < y.apas

bepa1 = Bepa([Apa([Bepa([])], [Cepa([])])])
bepa2 = Bepa([])

apa1 = Apa([bepa1], [])
apa2 = Apa([bepa2,bepa2], [Cepa([apa1])])

actor main(env):
    if apa1 == apa1 and apa1 != apa2 and bepa1 == bepa1 and bepa1 != bepa2:
        print("Ok")
        env.exit(0)
    else:
        print("Error")
        env.exit(1)
