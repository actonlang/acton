class Apa ():
    def __init__(self, bepas: list[Bepa]):
        self.bepas = bepas

class Bepa ():
    def __init__(self, apas: list[Apa], cepas: list[Cepa]):
        self.apas = apas
        self.cepas = cepas

class Cepa ():
    def __init__(self, bepas: list[Bepa]):
        self.bepas = bepas
    
extension Apa (Ord):
    def __eq__(x, y):
        return x.bepas == y.bepas
    def __lt__(x, y):
        return x.bepas < y.bepas

extension Bepa (Ord):
    def __eq__(x, y):
        return x.apas == y.apas and x.cepas == y.cepas
    def __lt__(x, y):
        return x.apas < y.apas and x.cepas < y.cepas

extension Cepa (Ord):
    def __eq__(x, y):
        return x.bepas == y.bepas
    def __lt__(x, y):
        return x.bepas < y.bepas

bepa1 = Bepa([Apa([Bepa([],[])])], [])
bepa2 = Bepa([], [Cepa([bepa1])])

apa1 = Apa([bepa1])
apa2 = Apa([bepa2,bepa2])

actor main(env):
    if apa1 == apa1 and apa1 != apa2 and bepa1 == bepa1 and bepa1 != bepa2:
        print("Ok")
        env.exit(0)
    else:
        print("Error")
        env.exit(1)
