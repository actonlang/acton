class Apa [X] ():
    def __init__(self, a: X, apas: list[Apa[X]]):
        self.a = a
        self.apas = apas

class Bepa ():
    def __init__(self, strings: list[str]):
        self.strings = strings

extension Apa [X(Ord)] (Ord):
    def __eq__(x, y):
        return x.a == y.a and x.apas == y.apas
    def __lt__(x, y):
        return x.a < y.a and x.apas < y.apas

extension Bepa (Ord):
    def __eq__(x, y):
        return x.strings == y.strings
    def __lt__(x, y):
        return x.strings < y.strings


bepa1 = Bepa(["bepa", "apa"])
bepa2 = Bepa(["apa", "bepa"])

apa1 = Apa("1", [])
apa2 = Apa("2", [apa1,apa1])

actor main(env):
    if apa1 == apa1 and apa1 != apa2 and bepa1 == bepa1 and bepa1 != bepa2:
        print("Ok")
        env.exit(0)
    else:
        print("Error")
        env.exit(1)
