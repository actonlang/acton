import acton.rts
import time

actor Interruptor():
    def interrupt():
        acton.rts.gc()
        after 0.001: interrupt()
    interrupt()

actor main(env):
    def test():
        s = time.Stopwatch()
        acton.rts.sleep(0.1)
        d = s.elapsed()
        if d.to_float() < 0.1:
            print("Sleep seems to have been below 1 second :(")
            return 1
        else:
            return 0

#    i = Interruptor()
    r = test()
    exit(env.cap, r)
