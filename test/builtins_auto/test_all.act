def test_all1() -> bool:
    stuff = [True, True, True]
    if all(stuff):
        return True
    print("all stuff is True but all() returned False")
    return False

def test_all2() -> bool:
    stuff = [True, False, True]
    if all(stuff):
        print("not all stuff is True but all() returned True")
        return False
    return True

actor main(env):
    if not test_all1():
        env.exit(1)
    if not test_all2():
        env.exit(1)
    env.exit(0)
