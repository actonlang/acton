import qcheck

def prop_lshift_1(n,i):
    i1 = abs(i)
    return n << i1 == n * 2**bigint(i1)

def prop_lshift_rshift_1(n,i):
    i1 = abs(i)
    return (n << i1) >> i1 == n

actor main(env):
     qcheck.qcheck_bigint_int("lshift_1", prop_lshift_1)
     qcheck.qcheck_bigint_int("lshift_rshift_1", prop_lshift_rshift_1)
     env.exit(0)
