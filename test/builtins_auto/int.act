# TODO: test that division by zero raises ZeroDivisionError

def test_i16_divzero():
    n = i16(0)
    try:
        i16(3) / n
    except ZeroDivisionError:
        try:
            i16(3) // n
        except ZeroDivisionError:
            return
    raise testing.NotRaisesError("expected ZeroDivisionError for i16")

def test_i32_divzero():
