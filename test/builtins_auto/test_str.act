def test_endswith():
    s = "foobar"
    print("Testing str.endswith()")
    print(s.endswith("foobar"))
    print(s.endswith("bar"))
    print(s.endswith("banana"))
    print(s.endswith("longbanana"))
    return (s.endswith("foobar") and
            s.endswith("bar") and
            not s.endswith("banana") and
            not s.endswith("longbanana"))

