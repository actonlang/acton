
def test_str_find():
    s = "hello, world!"
    print("Testing str.find()")
    return (s.find("hello, world!") == 0 and
            s.find("world") == 7 and
            s.find("world", 0) == 7 and
            s.find("world", 8) == -1 and
            s.find("world", 0, 5) == -1 and
            s.find("world", 0, 7) == -1 and
            s.find("crap") == -1)

