import qcheck

def prop_bigint_xor_int(a: int, b: int):
    return a ^ b == int(bigint(a) ^ bigint(b))

def prop_bigint_and_int(a: int, b: int):
    return a & b == int(bigint(a) & bigint(b))

def prop_bigint_or_int(a: int, b: int):
    return a | b == int(bigint(a) | bigint(b))

def prop_bigint_xor_invert(a: bigint, b: bigint):
    return a ^ b == ~a ^ ~b 

def prop_bigint_deMorgan1(a: bigint, b: bigint):
    return a & b == ~(~a | ~b)

def prop_bigint_and_comm(a: bigint, b: bigint):
    return a & b == b & a

def prop_bigint_or_comm(a: bigint, b: bigint):
    return a | b == b | a

def prop_bigint_xor_comm(a: bigint, b: bigint):
    return a ^ b == b ^ a

def prop_bigint_and_unit(a: bigint):
    print(a,  a & bigint(-1))
    return a & bigint(-1) == a

def prop_bigint_or_unit(a: bigint):
    return a | 0 == a
  
def prop_bigint_xor_inv(a: bigint):
    return a ^ a == 0

a : bigint = 12345676543456787654567898765456787654345676654
