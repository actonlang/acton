def test_i16():
    if not test_i16_divzero():
        return False
    x: i16 = 0
    if (bool(x != 0)):
       print("unexpected: bool(x) != 0")
       return False
    if x != 0:
       print("unexpected: x != 0")
       return False
    if i16("1234") != 1234:
        print('i16("1234") gives wrong result')
        return False
    if i16("0x1234") != 4660:
        print('i16("0x1234") gives wrong result')
        return False
    if i16("0o1234") != 668:
        print('i16("0o1234") gives wrong result')
        return False
    if i16("0b1111") != 15:
        print('i16("0b1111") gives wrong result')
        return False
    if i16("1234",7) != 466:
        print('i16("1234",7) gives wrong result')
        return False
    #if i16(2**15-1) + 1 != -2**15:
    #    print("i16 wraparound failed")
    #    return False
    return True

