actor main(env):

    def assert_eq(a,b):
        if a != b:
            print(a,'!=', b)
            env.exit(1)
    a : u64 = 37
    assert_eq(a+5,42)
    b = u64(0xffffffffffffffff)  # maximal value
    assert_eq(b+1,0)
    assert_eq(b//2*2,u64(18446744073709551614))
#    TODO: this should work but currently doesn't, probably because of some UB
#    sanitization...
#    print(b/2*2)
    assert_eq(u64("18446744073700000000"),u64(18446744073700000000))
    assert_eq(a-50,u64(18446744073709551603))                    # underflow
    c : int = 7
    d = {a:c, 3:1}
    assert_eq(a ^ 14, 43)
    assert_eq(a | 14, 47)
    assert_eq(~a, u64(18446744073709551578))
    print(d[37])  #assert_eq gives infinite loop during compilation
    for i in d.keys():
        print(i)

    a16 : i16 = 32
    print(a16*8)
    b16 : i16 = 12345
    print (a16 ^ b16)

    c32 : u32 = 0xfffff
    print(c32-100)
    print(c32 < 100000)
    f : u32 = 0-1
    print(f/1000000)
    env.exit(0)

