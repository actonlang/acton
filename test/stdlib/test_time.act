import time

actor main(env):
    et = time.Instant(int(env.argv[1]), 0, time.RealtimeClock(0, 0, 0))
    n = time.time()
    d = n.since(et)
    print("duration:", d)

    if abs(d.second) > 5:
        print("ERROR: diff seems too large, time.time*() should be roughly same as provided UNIX timestamp input")
        exit(env.cap, 1)

    exit(env.cap, 0)
