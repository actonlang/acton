class BaseA:
    x: int
    def __init__(self):
        self.x = 1

class BaseB(BaseA):
    y: int

    def __init__(self):
        BaseA.__init__(self)
        self.y = 1

class TestClass(BaseB):
    def __init__(self):
        BaseA.__init__(self)
        # Should get a failure since y is not initialized

actor main(env):
    t = TestClass()
    env.exit(0)
